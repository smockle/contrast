{
  "name": "@smockle/contrast",
  "version": "4.0.15",
  "description": "Analyse luminosity contrast ratio",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index",
  "types": "dist/lib/index.d.ts",
  "bin": {
    "contrast": "dist/bin/index.js"
  },
  "scripts": {
    "build": "bash scripts/build.sh",
    "build:windows": "call scripts/build.bat",
    "start": "node -r @std/esm dist/bin/index.mjs",
    "debug": "node --nolazy --inspect-brk=5858 -r @std/esm dist/bin/index.mjs",
    "test": "jest",
    "_version": "bash scripts/version.sh",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/contrast.git"
  },
  "author": "Clay Miller <clay@smockle.com> (https://www.smockle.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smockle/contrast/issues"
  },
  "homepage": "https://github.com/smockle/contrast#readme",
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.0.3",
    "codecov": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "ts-jest": "^22.4.1",
    "typescript": "^2.8.0-rc"
  },
  "lint-staged": {
    "*.{js,mjs,ts}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "@std/esm": {
    "cjs": true
  },
  "dependencies": {
    "@std/esm": "^0.26.0"
  }
}
