{
  "name": "@smockle/contrast",
  "version": "3.0.0",
  "description": "Analyse luminosity contrast ratio",
  "main": "lib/index.js",
  "bin": {
    "contrast": "./index.js"
  },
  "scripts": {
    "start": "node --harmony ./index.js",
    "test": "node --harmony test/index.js | colortape",
    "coverage": "nyc --reporter=lcov node --harmony test/index.js | colortape",
    "postcoverage": "nyc report",
    "codeclimate": "codeclimate-test-reporter < ./coverage/lcov.info",
    "prepublish": "npm test",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/contrast.git"
  },
  "author": "Clay Miller <clay@smockle.com> (https://smockle.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smockle/contrast/issues"
  },
  "homepage": "https://github.com/smockle/contrast#readme",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.0",
    "colortape": "^0.1.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.0",
    "nyc": "^10.3.2",
    "prettier-eslint-cli": "^4.0.3"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {}
}
